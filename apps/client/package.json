{
  "name": "@yellowipe-linx/client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode test",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "npm run build && dotenv -e .env.production -- netlify deploy --dir=apps/client/dist --site=$NETLIFY_SITE_ID --auth=$NETLIFY_AUTH_TOKEN --prod --no-build",
    "deploy:dev": "npm run build:dev && dotenv -e .env.development -- netlify deploy --dir=apps/client/dist --site=$NETLIFY_SITE_ID --auth=$NETLIFY_AUTH_TOKEN --prod --no-build"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.12",
    "@yellowipe-linx/schemas": "workspace:*",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "netlify-cli": "^23.4.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}

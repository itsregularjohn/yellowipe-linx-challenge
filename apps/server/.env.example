# Copy this file to .env or .env.local and fill in your actual values

# Node environment (development, production, test)
NODE_ENV=development

# Server configuration
PORT=4000

# Authentication
JWT_SECRET=your_jwt_secret_here_at_least_32_characters

# Database (Prisma Accelerate URL)
DATABASE_URL=prisma+postgres://accelerate.prisma-data.net/?api_key=your_prisma_accelerate_api_key

# Email configuration (for auth flow)
GOOGLE_GMAIL_EMAIL=your-email@gmail.com
GOOGLE_APP_PASSWORD=your_google_app_password

# AWS S3 configuration (for file uploads)
AWS_S3_BUCKET_NAME=your-s3-bucket-name
AWS_PROFILE=your-aws-profile